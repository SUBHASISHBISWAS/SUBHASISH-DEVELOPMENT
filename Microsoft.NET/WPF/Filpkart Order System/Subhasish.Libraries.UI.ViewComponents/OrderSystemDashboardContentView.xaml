<UserControl x:Class="Subhasish.Libraries.UI.ViewComponents.OrderSystemDashboardContentView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Subhasish.Libraries.UI.ViewComponents"
             mc:Ignorable="d" 
             d:DesignHeight="644" d:DesignWidth="992">
    <Grid Background="White">
        <ContentControl x:Name="CUSTOMERES_CONTENT_REGION" HorizontalAlignment="Left" VerticalAlignment="Top" Height="228" Width="992">
            <local:CustomersContentView HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top"/>
        </ContentControl>
        <ContentControl  x:Name="ORDERS_CONTENT_REGION" HorizontalAlignment="Left" VerticalAlignment="Top" Height="411" Width="992" Margin="0,238,0,0">
            <local:OrdersContentView HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top"/>
        </ContentControl>


    </Grid>
</UserControl>
