<Window x:Class="CalculatorClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CalculatorClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBlock x:Name="numeratorText" HorizontalAlignment="Left" Margin="72,58,0,0" TextWrapping="Wrap" Text="Numerator" VerticalAlignment="Top"/>
        <TextBlock x:Name="denominatorText" HorizontalAlignment="Left" Margin="72,90,0,0" TextWrapping="Wrap" Text="Denominator" VerticalAlignment="Top"/>
        <TextBox x:Name="numeratorFiled" HorizontalAlignment="Left" Height="23" Margin="163,57,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="denominatorField" HorizontalAlignment="Left" Height="23" Margin="163,83,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <Button x:Name="divide" Content="Divide" HorizontalAlignment="Left" Margin="188,141,0,0" VerticalAlignment="Top" Width="75" Click="divide_Click"/>
        <TextBlock x:Name="resultText" HorizontalAlignment="Left" Margin="81,188,0,0" TextWrapping="Wrap" Text="Result" VerticalAlignment="Top"/>

    </Grid>
</Window>
